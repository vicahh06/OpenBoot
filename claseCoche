package com.mycompany.openboot;

/**
 *
 * @author vahh0
 */
public class Main {
    public static void main(String[] args) {
       Coche miCoche = new Coche();
       miCoche.incrementarPuertas();
        System.out.println(miCoche.puertas);
      
    }
    
}
    class Coche{
        public int puertas=0;
        
        public void incrementarPuertas(){
            this.puertas++;
        }
    }
