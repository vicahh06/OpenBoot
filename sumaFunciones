package com.mycompany.clases;

/**
 *
 * @author vahh0
 */
public class Clases {

    public static void main(String[] args) {
        int num1, num2,num3;
        num1=4;
        num2=5;
        num3=1;
        
        System.out.println(suma(num1, num2, num3));
    }
    
    public static int suma(int a, int b, int c){
        int resultado=0;
        resultado=a+b+c;
        return resultado;
    }
}
